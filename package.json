{
  "name": "task-tracker",
  "version": "3.0.3",
  "description": "An Angular and Node Express Task Tracker System",
  "main": "index.js",
  "scripts": {
    "install:all": "npm install && cd ./public/task-app && npm install",
    "ng:start": "cd ./public/task-app && tsc && npm start",
    "node:start": "tsc-watch --onSuccess 'nodemon --exec babel-node ./bin/www'",
    "start": "concurrently \"npm run node:start\"  \"npm run ng:start\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/earlhansg/task-tracker.git"
  },
  "keywords": [
    "angular",
    "typescript",
    "express",
    "babel",
    "express typescript",
    "angular express"
  ],
  "author": "Earl Hans Pelin Genoso <earlhansg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-plugin-module-resolver": "^3.1.3",
    "bcrypt-nodejs": "0.0.3",
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "cors": "^2.8.4",
    "debug": "~2.6.3",
    "express": "^4.16.4",
    "morgan": "^1.9.1",
    "serve-favicon": "~2.4.2",
    "ts-node": "^6.0.0"
  },
  "devDependencies": {
    "@angular/cli": "^8.1.2",
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-typescript": "^7.1.0",
    "@babel/register": "^7.0.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/express-session": "^1.15.8",
    "@types/jasmine": "^2.8.7",
    "@types/morgan": "^1.7.35",
    "@types/node": "^7.0.60",
    "@types/passport": "^0.4.5",
    "@types/passport-google-oauth": "^1.0.38",
    "concurrently": "^3.5.0",
    "nodemon": "^1.18.9",
    "tsc-watch": "^1.0.21",
    "tslint": "^5.9.1",
    "typescript": "^3.4.5",
    "typescript-register": "^1.1.0"
  }
}
